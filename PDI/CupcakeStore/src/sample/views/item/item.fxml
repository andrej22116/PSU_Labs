<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="BOTTOM_LEFT" maxHeight="40.0" maxWidth="500.0" minWidth="500.0" prefHeight="40.0" prefWidth="500.0">
         <children>
            <Label fx:id="cupcakeName" prefHeight="47.0" prefWidth="118.0" text="Label" textAlignment="CENTER" textOverrun="CLIP" underline="true">
               <textFill>
                  <LinearGradient endX="1.0" endY="1.0" startX="1.0">
                     <stops>
                        <Stop color="WHITE" />
                        <Stop color="WHITE" offset="0.007662835249042145" />
                        <Stop color="#ff96cb" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </textFill>
               <font>
                  <Font name="System Bold" size="32.0" />
               </font>
               <effect>
                  <InnerShadow blurType="ONE_PASS_BOX" choke="0.17" color="#ff0db8" />
               </effect>
            </Label>
            <Label fx:id="cupcakeInfo" text="Suka" textFill="#c64fff">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</fx:root>
